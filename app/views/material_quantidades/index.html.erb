<p id="notice"><%= notice %></p>

<h1>Quantidades de materiais</h1>

<table>
  <thead>
    <tr>
      <th>Material</th>
      <th>Quantidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @material_quantidades.each do |material_quantidade| %>
      <tr>
        <td><%= material_quantidade.material %></td>
        <td><%= material_quantidade.quantidade %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Voltar', root_path%>
